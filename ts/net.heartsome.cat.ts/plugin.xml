<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.heartsome.cat.ts.Application">
         </run>
      </application>
   </extension>
   <extension
         id="lite_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="net.heartsome.cat.ts.application"
            name="Heartsome Translation Studio">
         <property
               name="appName"
               value="Heartsome Translation Studio">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="windowImages"
               value="images/xliff_16.png,images/xliff_32.png,images/xliff_48.png,images/xliff_64.png,images/xliff_128.png">
         </property>
         <property
               name="startupProgressRect"
               value="0,225,425,15">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="0,250,445,20">
         </property>
         <property
               name="aboutImage"
               value="images/xliff_128.png">
         </property>
         <property
               name="aboutText"
               value="%aboutL">
         </property>
      </product>
   </extension>
